{
    "configurations": [
        {
            "name": "scraper",
            "program": "${workspaceFolder}/lambda-functions/scraper/app.js",
            "request": "launch",
            "runtimeExecutable": "nodemon",
            "type": "node",
            "console": "integratedTerminal",
            "cwd":"${workspaceFolder}/lambda-functions/scraper"
        },
        {
            "type": "aws-sam",
            "request": "direct-invoke",
            "name": "api-genie:LoginUser",
            "invokeTarget": {
                "target": "template",
                "templatePath": "${workspaceFolder}/lambda-functions/api-genie/template.yaml",
                "logicalId": "LoginUser"
            },
            "lambda": {
                "payload": {},
                "environmentVariables": {}
            }
        },
        {
            "type": "aws-sam",
            "request": "direct-invoke",
            "name": "api-genie:HelloWorldFunction (nodejs18.x)",
            "invokeTarget": {
                "target": "template",
                "templatePath": "${workspaceFolder}/lambda-functions/api-genie/template.yaml",
                "logicalId": "HelloWorldFunction"
            },
            "lambda": {
                "payload": {},
                "environmentVariables": {},
                "runtime": "nodejs18.x"
            }
        },
        {
            "type": "aws-sam",
            "request": "direct-invoke",
            "name": "API api-genie:HelloWorldFunction (nodejs18.x)",
            "invokeTarget": {
                "target": "api",
                "templatePath": "${workspaceFolder}/lambda-functions/api-genie/template.yaml",
                "logicalId": "HelloWorldFunction"
            },
            "api": {
                "path": "/hello",
                "httpMethod": "get",
                "payload": {
                    "json": {}
                }
            },
            "lambda": {
                "runtime": "nodejs18.x"
            }
        }
    ]
}